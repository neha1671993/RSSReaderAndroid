<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rssnews"
    android:versionCode="1"
    android:versionName="1.2" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="8" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        name="com.rssnews.ApplicationContextProvider"
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/android:Theme.Light" >
        <activity
            android:name="com.rssnews.SplashScreen"
            android:label="@string/app_name"
            android:theme="@style/android:Theme.Light.NoTitleBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.rssnews.RSSNewsReaderPBActivity"
            android:label="@string/app_name"
            android:theme="@style/android:Theme.Light.NoTitleBar" />
        <activity
            android:name="com.rssnews.India_News"
            android:theme="@style/android:Theme.Light.NoTitleBar" />
        <activity
            android:name="com.rssnews.World_News"
            android:theme="@style/android:Theme.Light.NoTitleBar" />
        <activity
            android:name="com.rssnews.Favourites_News"
            android:theme="@style/android:Theme.Light.NoTitleBar" />
        <activity
            android:name="com.rssnews.RSS_News"
            android:theme="@style/android:Theme.Light.NoTitleBar" />
        <activity android:name="com.rssnews.DisPlayWebPageActivity" />
        <activity
            android:name="com.rssnews.ListRSSItemsActivity"
            android:theme="@style/android:Theme.Light.NoTitleBar" />
        <activity android:name="com.rssnews.RSSDatabaseHandler" />
        <activity android:name="com.rssnews.RSSFeed" />
        <activity android:name="com.rssnews.RSSItem" />
        <activity android:name="com.rssnews.RSSParser" />
        <activity android:name="com.rssnews.WebSite" />
        <activity
            android:name="com.rssnews.WorldListRSSItemsActivity"
            android:theme="@style/android:Theme.Light.NoTitleBar" />
        <activity
            android:name="com.rssnews.AddNewFeed"
            android:theme="@style/android:Theme.Light.NoTitleBar" />
        <activity
            android:name="com.rssnews.About"
            android:theme="@style/android:Theme.Light.NoTitleBar" />
        <activity
            android:name="com.rssnews.RSSListRSSItemsActivity"
            android:theme="@style/android:Theme.Light.NoTitleBar" />
        <activity
            android:name="com.rssnews.FavListRSSItemsActivity"
            android:theme="@style/android:Theme.Light.NoTitleBar" />
        <activity android:name="com.rssnews." />
    </application>

</manifest>